	<script type="text/javascript">
		$(document).ready(function() {
			// Í∏ÄÍº¥ Î¶¨Ïä§Ìä∏
			var fontList = ['ÎßëÏùÄ Í≥†Îîï','Íµ¥Î¶º','ÎèãÏõÄ','Î∞îÌÉï','Í∂ÅÏÑú','Arial','Courier New','Tahoma'];

			// Ìà¥Î∞î
			var toolbar = [
			    // Í∏ÄÍº¥ ÏÑ§Ï†ï
			    ['fontname', ['fontname']],
			    // Í∏ÄÏûê ÌÅ¨Í∏∞ ÏÑ§Ï†ï
			    ['fontsize', ['fontsize']],
			    // ÍµµÍ∏∞, Í∏∞Ïö∏ÏûÑÍº¥, Î∞ëÏ§Ñ,Ï∑®ÏÜå ÏÑ†, ÏÑúÏãùÏßÄÏö∞Í∏∞
			    ['style', ['bold', 'italic', 'underline','strikethrough', 'clear']],
			    // Í∏ÄÏûêÏÉâ
			    ['color', ['forecolor','color']],
			    // ÌëúÎßåÎì§Í∏∞
			    ['table', ['table']],
			    // Í∏ÄÎ®∏Î¶¨ Í∏∞Ìò∏, Î≤àÌò∏Îß§Í∏∞Í∏∞, Î¨∏Îã®Ï†ïÎ†¨
			    ['para', [/* 'ul', 'ol',  */'paragraph']],
			    // Ï§ÑÍ∞ÑÍ≤©
			    ['height', ['height']],
			    // Í∑∏Î¶ºÏ≤®Î∂Ä, ÎßÅÌÅ¨ÎßåÎì§Í∏∞, ÎèôÏòÅÏÉÅÏ≤®Î∂Ä
			    ['insert',['picture','link','video']],
			    // ÏΩîÎìúÎ≥¥Í∏∞, ÌôïÎåÄÌï¥ÏÑúÎ≥¥Í∏∞, ÎèÑÏõÄÎßê
			    ['view', ['codeview','fullscreen', 'help']]
			]
			
			// Í∏∞Î≥∏ÏÑ§Ï†ï
			var setting = {
	            height : 500,
	            minHeight : 500,
	            maxHeight : 1000,
	            width : 830,
	            focus : true,
	            lang : 'ko-KR',
	            fontNames: fontList,
	            fontNamesignoreCheck: fontList,
	            placeholder: 'ÎÇ¥Ïö©ÏùÑ ÏûÖÎ†•ÌïòÏÑ∏Ïöî.',
	            toolbar : toolbar,
	            callbacks : { // ÏΩúÎ∞± Ìï®Ïàò
	            	// Ïù¥ÎØ∏ÏßÄ ÏóÖÎ°úÎìú Ïãú ÎèôÏûë
		            onImageUpload : function(files, editor, welEditable) {
			            // ÌååÏùº ÏóÖÎ°úÎìú(Îã§Ï§ëÏóÖÎ°úÎìúÎ•º ÏúÑÌï¥ Î∞òÎ≥µÎ¨∏ ÏÇ¨Ïö©)
		            	for (var i = files.length - 1; i >= 0; i--) {
				            uploadSummernoteImageFile(files[i],
				            this);
				      	}
		            }
	            }
	        };
	
			// ÏÑ§Ï†ïÏùÑ Ï∂îÍ∞Ä
	        $('#summernote').summernote(setting);
		});
		
		
		function uploadSummernoteImageFile(file, el) {
			data = new FormData();
			data.append("file", file);
			$.ajax({
				data : data,
				type : "POST",
				url : "uploadSummernoteImageFile",
				contentType : false,
				enctype : 'multipart/form-data',
				processData : false,
				success : function(data) {
					$(el).summernote('editor.insertImage', data.url);
				}
			});
		}


		// ÏûëÏÑ± Ï§ë ÌéòÏù¥ÏßÄÎ•º Î≤óÏñ¥ÎÇ† Ïãú,
